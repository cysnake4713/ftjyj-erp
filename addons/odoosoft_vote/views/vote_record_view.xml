<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="tree_vote_vote_record_tree" model="ir.ui.view">
            <field name="name">view.odoosoft.vote.vote.record.tree</field>
            <field name="model">odoosoft.vote.vote.record</field>
            <field name="priority">16</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="record"/>
                    <field name="score"/>
                </tree>
            </field>
        </record>

        <record id="tree_vote_vote_record_form" model="ir.ui.view">
            <field name="name">view.odoosoft.vote.vote.record.form</field>
            <field name="model">odoosoft.vote.vote.record</field>
            <field name="priority">16</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="record"/>
                            <field name="files" widget="many2many_binary"/>
                            <field name="score"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</openerp>